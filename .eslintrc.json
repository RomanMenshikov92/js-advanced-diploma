{
  "extends": "airbnb-base",
  "env": {
    "es2021": true,
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "linebreak-style": 0
  }
}
